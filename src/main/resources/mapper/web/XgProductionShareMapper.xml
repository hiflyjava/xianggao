<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="cc.mrbird.web.dao.XgProductionShareMapper" >
  <resultMap id="BaseResultMap" type="cc.mrbird.web.domain.XgProductionShare" >
    <id column="id" property="id" jdbcType="BIGINT" />
    <result column="p_id" property="pId" jdbcType="BIGINT" />
    <result column="user_id" property="userId" jdbcType="BIGINT" />
    <result column="username" property="username" jdbcType="VARCHAR" />
    <result column="type" property="type" jdbcType="VARCHAR" />
    <result column="create_date" property="createDate" jdbcType="TIMESTAMP" />
    <result column="user_img" property="userImg" jdbcType="VARCHAR" />
    <result column="extend2" property="extend2" jdbcType="VARCHAR" />
  </resultMap>
  <sql id="Base_Column_List" >
    id, p_id, user_id, username, type, create_date, user_img, extend2
  </sql>
  <select id="getProductionShareByItems" resultMap="BaseResultMap">

    select  * from xg_production_share
    <where>

      <if test="startTime != null and startTime != '' and endTime !=null and endTime != '' ">
        and create_date  &gt;= #{startTime} and  create_date &lt;= #{endTime}
      </if>

    </where>



  </select>




</mapper>